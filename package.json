{
  "name": "promptini",
  "version": "1.0.0",
  "description": "Promptini - Universal LLM Chat Interface",
  "main": "index.js",
  "scripts": {"dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd server && npm run dev",
    "client": "cd client && npm start",
    "install-all": "npm install && cd server && npm install && cd ../client && npm install",
    "build": "cd client && npm run build",
    "check-ollama": "node check-ollama.js"
  },
  "keywords": ["promptini", "llm", "chat", "ollama", "ai", "universal"],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
